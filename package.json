    "jimp": "^0.22.10",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1"
  }
}
