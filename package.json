{
  "name": "bogo-cat-meme-app",
  "version": "1.0.0",
  "description": "A Node.js application for uploading images and turning them into animated Bongo Cat GIFs",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "setup": "./setup.sh",
    "run": "./run.sh",
    "run:prod": "./run.sh -m production",
    "run:dev": "./run.sh -m development",
    "run:verbose": "./run.sh -v"
  },
  "keywords": [
    "bongo",
